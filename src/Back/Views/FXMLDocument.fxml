      <ButtonBar layoutX="91.0" layoutY="34.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" text="Button" />
        </buttons>
      </ButtonBar>
    </children>
</AnchorPane>
